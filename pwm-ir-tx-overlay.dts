/dts-v1/; /plugin/;

/ {
	compatible = "allwinner,sun4i-a10";

	fragment@0 {
		target = <&pwm>;
		__overlay__ {
			pinctrl-names = "default";
			pinctrl-0 = <&pwm0_pin>, <&pwm1_pin>;
			status = "okay";
		};
	};

	fragment@1 {
		target-path = "/";
		__overlay__ {
			pwm-ir-transmitter {
				compatible = "pwm-ir-tx";
				pwms = <&pwm 0 25000 0>;
			};
		};
	};

};
